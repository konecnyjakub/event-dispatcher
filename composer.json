{
    "name": "konecnyjakub/event-dispatcher",
    "description": "A simple PSR-14 event dispatcher",
    "license": "BSD-3-Clause",
    "keywords": [
        "event dispatcher",
        "event listener",
        "events",
        "simple",
        "psr-14"
    ],
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "sources": "https://gitlab.com/konecnyjakub/event-dispatcher",
        "issues": "https://gitlab.com/konecnyjakub/event-dispatcher/-/issues"
    },
    "require": {
        "php": ">=8.3.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "php-parallel-lint/php-console-highlighter": "^0.5",
        "squizlabs/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^1.12",
        "konecnyjakub/mytester": "^6.0"
    },
    "provide": {
        "psr/event-dispatcher-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Konecnyjakub\\EventDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konecnyjakub\\EventDispatcher\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
